        }

        .close-btn {
            position: absolute;
            top: 10px;
            right: 15px;
            background: none;
            border: none;
            font-size: 24px;
            color: var(--text-dark);
            cursor: pointer;
            padding: 0;
        }

        .inquire-header {
            text-align: center;
            font-weight: 600;
            color: var(--text-dark);
            margin-bottom: 1rem;
            font-size: 1.2rem;
        }

        .inquire-options a {
            flex: 1; 
            white-space: nowrap;
            font-size: 1rem;
            padding: 1rem 0.75rem;
            text-shadow: none;
            transition: transform 0.2s, background 0.3s;
        }
        
        .inquire-options .btn-whatsapp {
            background: #25D366 !important; 
            color: white !important;
            border: 2px solid #25D366;
        }

        /* Email Button Styling */
        .inquire-options .btn-email {
            background: var(--card-background) !important;
            color: var(--text-dark) !important;
            border: 2px solid var(--primary-red) !important;
        }
        
        .inquire-options .btn-email:hover {
            background: var(--primary-red-hover) !important;
            color: white !important;
        }
        
        .inquire-options a:hover {
            transform: translateY(-2px);
        }

        .inquire-options a i {
            margin-right: 5px;
        }
    
/* ============================= */
/* FOOTER CREDIT STYLES */
/* ============================= */
.footer-credit {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    background: transparent;
    padding: 20px 40px;
    text-align: center;
    z-index: 10;
    opacity: 0;
    transform: translateY(15px);
