                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
                max-width: 200px;
            }

            .logo-image img {
                height: 34px;
                margin-bottom: 21px;
            }
            .main-nav-wrapper {
                margin: -12px 12px 0 12px;
                border-radius: 25px;
            }

            .navbar-container {
                padding: 0 12px;
                min-height: 45px;
            }

            .navbar-menu {
                top: 95px;
                width: calc(100% - 24px);
                margin: 0 12px;
            }

            .social-circle {
                width: 32px;
                height: 32px;
                font-size: 12px;
            }

            .page-header {
                padding: 1rem;
                margin-top: 10px;
            }

            .page-header h1 {
                font-size: 1.5rem;
            }

            .page-header p {
                font-size: 0.9rem;
            }

            .breadcrumb {
                padding: 1rem 5%;
                font-size: 0.85rem;
            }

            .container {
                padding: 1rem;
            }

            .products-grid {
                grid-template-columns: 1fr;
                gap: 1.5rem;
            }

            .product-actions {
                flex-direction: column;
            }

            .search-bar-wrapper {
                padding: 0;
            }

            #productSearch {
                padding: 0.75rem 1rem;
                font-size: 1rem;
            }
        }

        /* --- INQUIRE OPTIONS STYLES (Dual Button Popover) --- */
        
        .inquire-options {
            display: none; 
            position: fixed;
            /* Center on screen */
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 90%;
            max-width: 500px;
            min-height: 250px; /* Big square box */
            z-index: 1000; 
            padding: 30px;
            background: var(--card-background);
            border-radius: 12px;
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.5);
            
            /* Vertical Layout */
            flex-direction: column;
            gap: 1.5rem;
